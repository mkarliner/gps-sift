{
	"name": "gps-sift",
	"display-name": "GPS Sift",
	"description": "",
	"author": "",
	"version": "1.0.0",
	"icon": "assets/blueprint.svg",
	"interfaces": {
		"email": {
			"thread": {
				"controller": "dist/js/controller.umd-es2015.js"
			},
			"client": {
				"controller": "dist/js/email-client-controller.umd-es2015.js"
			}
		},
		"summary": {
			"controller": "dist/js/controller.umd-es2015.js"
		}
	},
	"dag": {
		"inputs": {
			"emails": {},
			"slack-bot": {},
			"webhooks": {
				"passiveeye": {
					"inbound": {
						"uri": "pe",
						"jsonPath": "{\"key\": \"$.body.device\", \"value\": $.body}"
					}
				},
				"owntracks": {
					"inbound": {
						"uri": "ot",
						"jsonPath": "{\"key\": \"$.body.tid\", \"value\": $.body}"
					}
				}
			}
		},



		"nodes": [{
			"#": "PassiveEye",
			"input": {
				"bucket": "passiveeye"
			},
			"implementation": {
				"javascript": "server/node1.js"
			},
			"outputs": {
				"positions": {},
				"devices": {}
			}
		},
		{
					"#": "OwnTracks",
					"input": {
						"bucket": "owntracks"
					},
					"implementation": {
						"javascript": "server/owntracks.js"
					},
					"outputs": {
						"positions": {},
						"devices": {}
					}
				},
		{
			"#": "Python Node",

			"implementation": {
				"javascript": "server/node1.js"
			},
			"input": {
				"bucket": "raw_positions"
			},
			"outputs": {
				"bounding": {}
			}
		}],
		"stores": {
			"raw_positions": {
				"key$schema": "string",
				"ttl": 86400
			}
		},
		"outputs": {
			"exports": {
				"bounding": {
					"key$schema": "string"
				},
				"devices": {
					"key$schema": "string"
				},
				"positions": {
					"key$schema": "string/string",
					"ttl": 86400
				}
			}
		}
	}
}
